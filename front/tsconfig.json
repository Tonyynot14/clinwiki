{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "baseUrl": "app",
    "jsx": "react",
    "downlevelIteration": true,
    "keyofStringsOnly": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "lib": [
      "es2017", 
      "dom",
      "esnext.asynciterable"
    ]
  },
  "exclude": ["node_modules"]
}
